@article{Wellander2002,
author = {Wellander, N.}, 
journal = {Applications of Mathematics},
title = {Homogenization of the Maxwell equations: Case II. Nonlinear conductivity},
abstract = {The Maxwell equations with uniformly monotone nonlinear electric conductivity in a heterogeneous medium, which may be non-periodic, are homogenized by two-scale convergence. We introduce a new set of function spaces appropriate for the nonlinear Maxwell system. New compactness results, of two-scale type, are proved for these function spaces. We prove existence of a unique solution for the heterogeneous system as well as for the homogenized system. We also prove that the solutions of the heterogeneous system con- verge weakly to the solution of the homogenized system. Furthermore, we prove corrector results, important for numerical implementations.},
